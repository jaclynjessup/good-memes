<div>
<h1><%= @user.username %>'s profile</h1>
<img class="profile_photo", src="<%= @user.avatar %>">
</div>
<ul id="main-page">
  <h3><%= @user.username %>'s memes:</h3>
  <% @memes.each do |meme| %>
    <li><a id="main-list-items" href="/memes/<%= meme.id %>"><img width="100", height="100", src='<%= meme.img_url %>'></a></li>
  <% end %>
</ul>
