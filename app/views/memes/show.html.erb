<div class="meme_show">
<h1><%= @meme.name %></h1>
<img src="<%= @meme.img_url%>">
<p>Created by: <%= @meme.user.email %></p>

<%= render "reviews/new" %>

<h3>Reviews of <%= @meme.name %>:</h3>
<ul class="reviews">
  <% @reviews.each do |review| %>
    <li><% review.rating.times do %>♨️<% end %> - <%= review.body %> Posted by: <%= review.user.email %></li>
  <% end %>
<ul>
</div>
